{
  "name": "recipemobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "emu": "start-android-emulator",
    "clean": "cd android && gradlew clean",
    "signReport": "cd android && gradlew signingReport",
    "apk": "cd android && gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "aab": "cd android && gradlew bundleRelease",
    "both": "cd android && gradlew assembleRelease bundleRelease -x bundleReleaseJsAndAssets",
    "assetsBundle": " react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-picker/picker": "^2.5.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.5.0",
    "lottie-react-native": "^6.2.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-image-picker": "^5.6.1",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-onesignal": "^4.5.1",
    "react-native-popup-confirm-toast": "^2.3.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-toast-message": "^2.1.6",
    "react-redux": "^8.1.2",
    "readme-md-generator": "^1.0.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
